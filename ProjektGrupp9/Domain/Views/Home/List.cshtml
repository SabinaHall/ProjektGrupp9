@model IEnumerable<DataLogic.Models.ApplicationUser>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        
        <td>
            @Html.ActionLink("Redigera", "EditProfile", new { id=item.Id }) |
            @Html.ActionLink("Detaljer", "ProfilePage", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
